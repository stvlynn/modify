{
  "expo": {
    "name": "Dify Mobile",
    "slug": "dify-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/welcome/icons/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/welcome/icons/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dify.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/welcome/icons/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dify.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
